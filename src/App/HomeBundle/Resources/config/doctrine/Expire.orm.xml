<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\HomeBundle\Entity\Expire" table="expire">
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="user" type="integer" column="User" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="package" type="integer" column="Package" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="interest" type="string" column="Interest" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="enddate" type="string" column="Enddate" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
